<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 xmlns:vm="clr-namespace:Fuse.ViewModels"
             x:Class="Fuse.Views.DebugView"
			 x:DataType="vm:DebugViewModel">
	<StackPanel>
		<Button Command="{Binding LoadAppSettingsCommand}" Content="load AppSettings"/>
		
		<Button Command="{Binding SaveAppSettingsCommand}" Content="Save AppSettings"/>
		<Button Command="{Binding InsertGameInstallIntoAppSettingsCommand}" Content="Insert GameInstall Into AppSettings"/>
		<Button Command="{Binding LoadCueCommand}" Content="load Cue4parse"/>
		<Button Command="{Binding TextCommand}" Content="FunnyText"/>
		<Button Command="{Binding GimmeAthenaTerrainCommand}" Content="Athena my beloved"/>
		<Button Command="{Binding ExportPOICommand}" Content="Export POI"/>
		<Image Source="/Assets/pig.png" MaxHeight="200"/>


		
	</StackPanel>
	
</UserControl>
